{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 8
    },
    "rules": {
      "no-console": "off",
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "object-curly-newline": 0,
      "react/self-closing-comp": 0
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    }
}